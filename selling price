import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	   System.out.println("Cost price: ");
	   int c=sc.nextInt();
	   System.out.println("Profit: ");
	   int p=sc.nextInt();
	  int result=c+p;
	   System.out.println("selling price: "+result);
}
}
